  "description": "Behat testing for Forms in Drupal",
  "keywords": [
    "drupal",
    "web",
    "test",
    "behat"
  ],
  "homepage": "http://drupal.org/project/behatdrupalforms",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Lisa Ridley",
      "email": "lhridley@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
